#!/bin/sh

# LAM MPI

echo 'Performing benchmarks for LAM MPI'
./benchlam standardPEC.igf pec
./benchlam standardDIEL.igf diel
./benchlam standard4DIEL.igf diel4
./benchlam standard9DIEL.igf diel9
./benchlam standard16DIEL.igf diel16

