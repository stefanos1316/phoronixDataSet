#!/usr/bin/python


def run(rep=1000):
    n = 1000
    for i in range(1, rep):
        a = 1
        b = 1
        for j in range(0, n):
            t = a
            a = b
            b = b+t
    print(b)

from sys import argv
rep = 1000
if(len(argv) > 1):
    rep = int(argv[1])
run(rep)



